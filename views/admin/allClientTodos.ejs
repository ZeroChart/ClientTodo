<div class="admin-title">
  <h2><%= locals.title %></h2>
  <a href="#" class="button">+ 새 할일</a>
</div>

<ul class="admin-posts">

  <% data.forEach(clienttodo => { %>
   
    <li>
      <a href="/post/<%= clienttodo._id %>">
        <%= clienttodo.title %> 
      </a>
      <div class="admin-post-controls">
        <a href="/edit/<%= clienttodo._id %>" class="btn">편집</a>

        <form action="/delete/<%= clienttodo._id %>?_method=DELETE" method="POST">
          <input type="submit" value="삭제" class="btn-delete btn">
        </form>
      </div>
    </li>

  <% }) %>
</ul>